{
  "name": "rusheet",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run build:wasm && vite build",
    "build:wasm": "wasm-pack build crates/rusheet-wasm --target web --out-dir ../../pkg",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:rust": "cargo test --all",
    "test:all": "pnpm run test:rust && pnpm test"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.16",
    "jsdom": "^27.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-top-level-await": "^1.4.0",
    "vite-plugin-wasm": "^3.3.0",
    "vitest": "^1.0.0"
  }
}
